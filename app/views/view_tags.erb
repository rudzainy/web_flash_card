<div class="container">
  <h1>View tags</h1>
  <p><%= @flag %></p>

  <table>
    <tr>
      <th>Tag: <%= @tag.name %></th>
    </tr>
    <% @tag.properties.each do |property| %>
    <tr>
      <td>
        <%= property.title %><br>
      </td>
    </tr>
    <% end %>
  </table>
  <a href="../../"><button>Main Page</button></a>
</div>
